@model WSClient.ApplicationWS.UserDTO[]

@{
    ViewData["Title"] = "Index";
}

<h1>Lista de Usuarios</h1>

@foreach (var user in Model)
{
    <div>
        @* <h4>UserDTO</h4 >*@
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => user.Email)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => user.Email)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => user.Name)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => user.Name)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => user.Password)
            </dt>
            <dd class="col-sm-10">
                ********
            </dd>
        </dl>
    </div>
    <div>
        @Html.ActionLink("Modificar", "ManageUser", new { id = user.Id }) |
        @Html.ActionLink("Eliminar", "Delete", new { id = user.Id })
        @* <a asp-action="Index">Back to List</a> *@
    </div>
}

